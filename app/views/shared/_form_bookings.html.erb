<%= simple_form_for [@bot, @booking], url: bot_bookings_path, method: :post  do |f| %>
  <br>
  <%= f.input :start, as: :date, required: false, input_html: {id: "range_start" } %>
  <br>
  <%= f.input :end, as: :date, required: false, input_html: {id: "range_end"} %>
  <br>
  <%= button_tag(type: 'submit', class: "btn btn-primary") do %>
    <i class="icon-ok icon-white"></i> Book
  <% end %>
<% end %>
